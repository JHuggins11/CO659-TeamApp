@page
@model CO659_TeamApp.Pages.Practitioners.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Practitioner[0].PractitionerFName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Practitioner[0].PractitionerLName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Practitioner[0].PractitionerTitle)
