            <td>
                @Html.DisplayFor(modelItem => item.PractitionerFName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PractitionerLName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PractitionerTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PractitionerDept)
            </td>
            <td>
